syntax "ARGS"

NUMBER:         "\<[-]?[0-9]*([Ee][+-]?[0-9]+)?\>"  "\<[-]?[0](\.[0-9]+)?\>"
STRING:         "[a-zA-Z]+[a-zA-Z0-9]*"
FUNCTION:       "\<(console|grab|inspect)\>"
NULL:           "\<null\>"
BOOLEAN:        "\<(true|false)\>"
VARIABLE:       "(\[|,)\s*[a-zA-Z0-9]*\s*:"
PLAIN:          "(:|\[|,|\])"
OPTION:         "\s+[-]{1,2}[a-zA-Z0-9\?]+(\s+|$)"
$BLOCK_COMMENT: "/*, */"
$LINE_COMMENT:  "//"
$BALANCED_DELIMITERS  "", '"
